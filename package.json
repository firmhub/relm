{
  "name": "relm",
  "version": "0.7.0-alpha1",
  "description": "Composable components",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcode/relm.git"
  },
  "scripts": {
    "yoyo:index": "rollup -c src/yoyo/rollup.js src/yoyo/index.js > yoyo/index.js",
    "yoyo:jsx": "rollup -c src/yoyo/rollup.js src/yoyo/jsx.js > yoyo/jsx.js",
    "yoyo:preset": "rollup -c src/yoyo/rollup.js src/yoyo/preset.js > yoyo/preset.js",
    "yoyo": "./parallel.sh 'npm run yoyo:index' 'npm run yoyo:jsx' 'npm run yoyo:preset'"
  },
  "keywords": [
    "frontend",
    "component",
    "framework",
    "react",
    "mithril",
    "redux",
    "elm",
    "architecture"
  ],
  "author": "Amandeep Singh Othi <a-s-o@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-s-o/relm/issues"
  },
  "homepage": "https://github.com/a-s-o/relm#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-plugin-lodash": "^3.2.0",
    "babel-preset-es2015": "^1.1.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "lodash": "^4.13.1",
    "npm-run-parallel": "^0.5.0",
    "rollup": "^0.33.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-node-resolve": "^1.7.1"
  },
  "dependencies": {
    "yo-yo": "^1.2.2",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0"
  }
}
