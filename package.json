{
  "name": "relm",
  "version": "1.0.0",
  "description": "Composable components",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcode/relm.git"
  },
  "scripts": {
    "test": "ava",
    "dev:quiz": "babel-node ./examples/popquiz/data.js; webpack-dev-server examples/popquiz/app.js --output-filename=app.dist.js --inline --content-base=examples/popquiz",
    "dev:http": "webpack-dev-server examples/http/app.js --output-filename=app.dist.js --inline --content-base=examples/http",
    "dev:routes": "webpack-dev-server examples/routes/app.js --output-filename=app.dist.js --inline --content-base=examples/routes",
    "dev:todo": "webpack-dev-server examples/todo/js/app.js --output-public-path=/js/ --output-filename=app.dist.js --inline --content-base=examples/todo",
    "watch": "webpack --watch",
    "build": "env NODE_ENV=production webpack --progress",
    "prepublish": "npm run build"
  },
  "keywords": [
    "frontend",
    "component",
    "framework",
    "react",
    "mithril",
    "redux",
    "elm",
    "architecture"
  ],
  "author": "Amandeep Singh Othi <a-s-o@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-s-o/relm/issues"
  },
  "homepage": "https://github.com/a-s-o/relm#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.0",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.8.0",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "csjs": "^1.0.5",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "inferno-create-element": "^0.7.16",
    "inferno-dom": "^0.7.16",
    "ini": "^1.3.4",
    "insert-css": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mithril": "^0.2.5",
    "npm-run-parallel": "^0.5.0",
    "postcss": "^5.1.1",
    "postcss-csso": "^1.1.2",
    "postcss-safe-parser": "^1.0.7",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "redux": "^3.5.2",
    "sheet-router": "^3.1.1",
    "tcomb": "^3.2.8",
    "webpack": "^2",
    "webpack-dev-server": "^2",
    "xhr": "^2.2.1",
    "yo-yo": "^1.2.2"
  },
  "dependencies": {
    "path-to-regexp": "^1.5.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.{js,jsx}"
    ]
  }
}
