{
  "name": "relm",
  "version": "1.0.0-beta",
  "description": "Composable components",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcode/relm.git"
  },
  "scripts": {
    "examples:todo": "webpack examples/todo/js/components.jsx examples/todo/js/components.dist.js --output-library=components",
    "build:examples": "./parallel.sh 'examples:todo'",
    "build:inferno": "webpack src/packages/inferno.js inferno.js --output-library=inferno",
    "build:react": "webpack src/packages/react.js react.js --output-library=react",
    "build:morphdom": "webpack src/packages/morphdom.js morphdom.js --output-library=morphdom",
    "build": "./parallel.sh 'build:examples' 'build:inferno -- --config=webpack.prod.js' 'build:react -- --config=webpack.prod.js' 'build:morphdom -- --config=webpack.prod.js'",
    "prepublish": "npm run build"
  },
  "keywords": [
    "frontend",
    "component",
    "framework",
    "react",
    "mithril",
    "redux",
    "elm",
    "architecture"
  ],
  "author": "Amandeep Singh Othi <a-s-o@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-s-o/relm/issues"
  },
  "homepage": "https://github.com/a-s-o/relm#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.2.0",
    "babel-preset-es2015": "^6.11.1",
    "babel-preset-react": "^6.11.1",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "classnames": "^2.2.5",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "inferno-create-element": "^0.7.16",
    "inferno-dom": "^0.7.16",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "mithril": "^0.2.5",
    "npm-run-parallel": "^0.5.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "redux": "^3.5.2",
    "webpack": "^1.13.1",
    "yo-yo": "^1.2.2"
  },
  "dependencies": {
  }
}
